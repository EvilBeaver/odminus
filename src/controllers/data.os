Функция Upload() Экспорт
	
	Если НЕ ЗапросHttp.Метод = "POST" Тогда
		Возврат КодСостояния(404);	
	КонецЕсли;
	
	Данные = ЗапросHttp.ДанныеФормы;

	ОписаниеОтвета = "";
	
	Загрузчик = Новый ЗагрузчикДанных();

	РезультатЗаписиДанных = Загрузчик.ЗагрузитьДанныеОтАгента(Данные["Content"]);
			
	Если РезультатЗаписиДанных Тогда
		Возврат КодСостояния(200);
	Иначе
		Возврат КодСостояния(400);
	КонецЕсли;
	
КонецФункции
